<h2>Edit Memory</h2>

<form method="post" action="/memories/<%=@memory.id%>">
  <input id="hidden" type="hidden" name="_method" value="PATCH">
  <input type="text" name="title" value="<%= @memory.title %>">
  <input type="text" name="content" value="<%= @memory.content %>">
  <input type="text" name="emotion" value="<%= @memory.emotion %>">
  <input type="text" name="player" value="<%= @memory.player %>">
  <input type="submit" value="submit">
</form>