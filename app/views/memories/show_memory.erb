<h4>New Memory:</h4>
<ul>
  <li><%= @memory.content %></li>
  <% @memory.emotions.each do |emotion| %>
    <li><%= emotion.feeling %></li>
</ul>


  <% end %>

<form method="post" action="/memories/<%=@memory.id%>/delete">
  <input id="hidden" type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete Memory">
</form> <br>

<form method="GET" action="/memories/<%=@memory.id%>/edit">
  <input type="submit" value="Edit Memory" />
</form> <br>

<form method="GET" action="/memories">
  <input type="submit" value="Your Memories Page" />
</form>